<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mountisome.paper_system.dao.RegisterMapper">

    <!--添加用户-->
    <insert id="addNewUser" parameterType="com.mountisome.paper_system.entity.User">
        insert into user(name,pwd,phone,mailbox,addtime) values (#{name},#{pwd},#{phone},#{mailbox},#{addtime})
    </insert>

</mapper>